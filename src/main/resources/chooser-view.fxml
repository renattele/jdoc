<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Text?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="jdoc.presentation.ChooserController" fx:id="container" style="-fx-background-color: -fx-surface">
    <padding>
        <Insets bottom="20.0" left="60.0" right="60.0" top="20.0"/>
    </padding>
    <Text style="-fx-font-size: 60">jdoc</Text>
    <TextField fx:id="connectField" promptText="Host URL" focusTraversable="false"/>
    <FlowPane fx:id="recentContainer" hgap="12" vgap="12" alignment="CENTER">

    </FlowPane>
</VBox>
